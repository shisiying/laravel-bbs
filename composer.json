{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.7",
        "erusev/parsedown": "^1.6",
        "fideloper/proxy": "~3.3",
        "fukuball/jieba-php": "^0.33.0",
        "guzzlehttp/guzzle": "~6.3",
        "hieu-le/active": "~3.5",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "~1.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/html-to-markdown": "^4.2",
        "mews/captcha": "~2.0",
        "mews/purifier": "~2.0",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/pinyin": "~3.0",
        "predis/predis": "~1.0",
        "qiniu/php-sdk": "^7.2",
        "rcrowe/turbo": "0.2.*",
        "socialiteproviders/weixin": "^4.0",
        "socialiteproviders/weixin-web": "^3.0",
        "spatie/laravel-permission": "~2.7",
        "spatie/laravel-pjax": "^1.3",
        "summerblue/administrator": "~1.1",
        "vanry/laravel-scout-tntsearch": "^1.2",
        "venturecraft/revisionable": "^1.28",
        "viacreative/sudo-su": "~1.1",
        "yansongda/laravel-pay": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "summerblue/generator": "~0.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "XHZ\\": "app/XHZ/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
